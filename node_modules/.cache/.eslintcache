{ id } = useParams();\n\n  useEffect(() => {\n    dispatch(getAllOrdersOfUser(user._id));\n  }, [dispatch]);\n\n  const data = orders && orders.find((item) => item._id === id);\n\n  const reviewHandler = async (e) => {\n    await axios\n      .put(\n        `${server}/product/create-new-review`,\n        {\n          user,\n          rating,\n          comment,\n          productId: selectedItem?._id,\n          orderId: id,\n        },\n        { withCredentials: true }\n      )\n      .then((res) => {\n        toast.success(res.data.message);\n        dispatch(getAllOrdersOfUser(user._id));\n        setComment(\"\");\n        setRating(null);\n        setOpen(false);\n      })\n      .catch((error) => {\n        toast.error(error);\n      });\n  };\n  \n  const refundHandler = async () => {\n    await axios.put(`${server}/order/order-refund/${id}`,{\n      status: \"Processing refund\"\n    }).then((res) => {\n       toast.success(res.data.message);\n    dispatch(getAllOrdersOfUser(user._id));\n    }).catch((error) => {\n      toast.error(error.response.data.message);\n    })\n  };\n\n  return (\n    <div className={`py-4 min-h-screen ${styles.section}`}>\n      <div className=\"w-full flex items-center justify-between\">\n        <div className=\"flex items-center\">\n          <BsFillBagFill size={30} color=\"crimson\" />\n          <h1 className=\"pl-2 text-[25px]\">Детали заказа</h1>\n        </div>\n      </div>\n\n      <div className=\"w-full flex items-center justify-between pt-6\">\n        <h5 className=\"text-[#00000084]\">\n          Заказ ID: <span>#{data?._id?.slice(0, 8)}</span>\n        </h5>\n        <h5 className=\"text-[#00000084]\">\n          Оформлен: <span>{data?.createdAt?.slice(0, 10)}</span>\n        </h5>\n      </div>\n\n      {/* order items */}\n      <br />\n      <br />\n      {data &&\n        data?.cart.map((item, index) => {\n          return(\n          <div className=\"w-full flex items-start mb-5\">\n            <img\n              src={`${item.images[0]?.url}`}\n              alt=\"\"\n              className=\"w-[80x] h-[80px]\"\n            />\n            <div className=\"w-full\">\n              <h5 className=\"pl-3 text-[20px]\">{item.name}</h5>\n              <h5 className=\"pl-3 text-[20px] text-[#00000091]\">\n                ₽{item.discountPrice} x {item.qty}\n              </h5>\n            </div>\n            {!item.isReviewed && data?.status === \"Delivered\" ?  <div\n                className={`${styles.button} text-center text-[#fff]`}\n                onClick={() => setOpen(true) || setSelectedItem(item)}\n              >\n                Оставить отзыв\n              </div> : (\n             null\n            )}\n          </div>\n          )\n         })}\n\n      {/* review popup */}\n      {open && (\n        <div className=\"w-full fixed top-0 left-0 h-screen bg-[#0005] z-50 flex items-center justify-center\">\n          <div className=\"w-[50%] h-min bg-[#fff] shadow rounded-md p-3\">\n            <div className=\"w-full flex justify-end p-3\">\n              <RxCross1\n                size={30}\n                onClick={() => setOpen(false)}\n                className=\"cursor-pointer\"\n              />\n            </div>\n            <h2 className=\"text-[30px] font-[500] font-Poppins text-center\">\n              Оставьте отзыв\n            </h2>\n            <br />\n            <div className=\"w-full flex\">\n              <img\n                src={`${selectedItem?.images[0]?.url}`}\n                alt=\"\"\n                className=\"w-[80px] h-[80px]\"\n              />\n              <div>\n                <div className=\"pl-3 text-[20px]\">{selectedItem?.name}</div>\n                <h4 className=\"pl-3 text-[20px]\">\n                  \n                  ₽{selectedItem?.discountPrice} x {selectedItem?.qty}\n                </h4>\n              </div>\n            </div>\n\n            <br />\n            <br />\n\n            {/* ratings */}\n            <h5 className=\"pl-3 text-[20px] font-[500]\">\n              Оценить <span className=\"text-red-500\">*</span>\n            </h5>\n            <div className=\"flex w-full ml-2 pt-1\">\n              {[1, 2, 3, 4, 5].map((i) =>\n                rating >= i ? (\n                  <AiFillStar\n                    key={i}\n                    className=\"mr-1 cursor-pointer\"\n                    color=\"rgb(246,186,0)\"\n                    size={25}\n                    onClick={() => setRating(i)}\n                  />\n                ) : (\n                  <AiOutlineStar\n                    key={i}\n                    className=\"mr-1 cursor-pointer\"\n                    color=\"rgb(246,186,0)\"\n                    size={25}\n                    onClick={() => setRating(i)}\n                  />\n                )\n              )}\n            </div>\n            <br />\n            <div className=\"w-full ml-3\">\n              <label className=\"block text-[20px] font-[500]\">\n                Написать комментарий \n                <span className=\"ml-1 font-[400] text-[16px] text-[#00000052]\">\n                  (Необязательно)\n                </span>\n              </label>\n              <textarea\n                name=\"comment\"\n                id=\"\"\n                cols=\"20\"\n                rows=\"5\"\n                value={comment}\n                onChange={(e) => setComment(e.target.value)}\n                placeholder=\"Как вы оценили товар? Напишите своё впечатление!\"\n                className=\"mt-2 w-[95%] border p-2 outline-none\"\n              ></textarea>\n            </div>\n            <div\n              className={`${styles.button} text-white text-[20px] ml-3`}\n              onClick={rating > 1 ? reviewHandler : null}\n            >\n              Отправить\n            </div>\n          </div>\n        </div>\n      )}\n\n      <div className=\"border-t w-full text-right\">\n        <h5 className=\"pt-3 text-[18px]\">\n          Итоговая цена: <strong>₽{data?.totalPrice}</strong>\n        </h5>\n      </div>\n      <br />\n      <br />\n      <div className=\"w-full 800px:flex items-center\">\n        <div className=\"w-full 800px:w-[60%]\">\n          <h4 className=\"pt-3 text-[20px] font-[600]\">Адрес доставки:</h4>\n          <h4 className=\"pt-3 text-[20px]\">\n            {data?.shippingAddress.address1 +\n              \" \" +\n              data?.shippingAddress.address2}\n          </h4>\n          <h4 className=\" text-[20px]\">{data?.shippingAddress.country}</h4>\n          <h4 className=\" text-[20px]\">{data?.shippingAddress.city}</h4>\n          <h4 className=\" text-[20px]\">{data?.user?.phoneNumber}</h4>\n        </div>\n        <div className=\"w-full 800px:w-[40%]\">\n          <h4 className=\"pt-3 text-[20px]\">Инфо. оплаты:</h4>\n          <h4>\n            Статус:{\" \"}\n            {data?.paymentInfo?.status ? data?.paymentInfo?.status : \"Не оплачен\"}\n          </h4>\n          <br />\n           {\n            data?.status === \"Delivered\" && (\n              <div className={`${styles.button} text-white text-center`}\n              onClick={refundHandler}\n              >Вернуть деньги</div>\n            )\n           }\n        </div>\n      </div>\n      <br />\n      <Link to={`/inbox?${user._id}`}>\n        <div className={`${styles.button} text-white text-center`}>Отправить сообщение</div>\n      </Link>\n      <br />\n      <br />\n    </div>\n  );\n};\n\nexport default UserOrderDetails;\n","/data/data/com.termux/files/home/commerce/frontend/src/components/Events/Events.jsx",["726"],[],"import React, { useEffect } from 'react'\nimport { useSelector } from 'react-redux';\nimport styles from '../../styles/styles'\nimport EventCard from \"./EventCard\";\n\nconst Events = () => {\n  const {allEvents,isLoading} = useSelector((state) => state.events);  \n   \n  return (\n    <div>\n     {\n      !isLoading && (\n        <div className={`${styles.section}`}>\n      <div className={`${styles.heading}`}>\n        <h1>Популярные события</h1>\n      </div>\n\n      <div className=\"w-full grid\">\n         {\n          allEvents.length !== 0 && (\n            <EventCard data={allEvents && allEvents[0]} />\n          )\n         }\n         <h4>{\n           allEvents?.length === 0 && (\n            'нет событий!'\n           )\n          }\n\n         </h4>\n      </div>\n     \n    </div>\n      )\n     }\n  </div>\n  )\n}\n\nexport default Events","/data/data/com.termux/files/home/commerce/frontend/src/components/Events/EventCard.jsx",[],[],"/data/data/com.termux/files/home/commerce/frontend/src/components/Shop/ShopLogin.jsx",["727"],[],"import { React, useEffect, useState } from \"react\";\nimport { AiOutlineEye, AiOutlineEyeInvisible } from \"react-icons/ai\";\nimport styles from \"../../styles/styles\";\nimport { Link, useNavigate } from \"react-router-dom\";\nimport axios from \"axios\";\nimport { server } from \"../../server\";\nimport { toast } from \"react-toastify\";\n\nconst ShopLogin = () => {\n  const navigate = useNavigate();\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const [visible, setVisible] = useState(false);\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n\n    await axios\n      .post(\n        `${server}/shop/login-shop`,\n        {\n          email,\n          password,\n        },\n        { withCredentials: true }\n      )\n      .then((res) => {\n        toast.success(\"Login Success!\");\n        navigate(\"/dashboard\");\n        window.location.reload(true); \n      })\n      .catch((err) => {\n        toast.error(err.response.data.message);\n      });\n  };\n\n  return (\n    <div className=\"min-h-screen bg-gray-50 flex flex-col justify-center py-12 sm:px-6 lg:px-8\">\n      <div className=\"sm:mx-auto sm:w-full sm:max-w-md\">\n        <h2 className=\"mt-6 text-center text-3xl font-extrabold text-gray-900\">