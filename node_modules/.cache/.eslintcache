             size={25}\n                  className=\"cursor-pointer\"\n                  onClick={() => setOpenCart(false)}\n                />\n              </div>\n              {/* Item length */}\n              <div className={`${styles.noramlFlex} p-4`}>\n                <IoBagHandleOutline size={25} />\n                <h5 className=\"pl-2 text-[20px] font-[500]\">{cart && cart.length} товаров</h5>\n              </div>\n\n              {/* cart Single Items */}\n              <br />\n              <div className=\"w-full border-t\">\n                {cart &&\n                  cart.map((i, index) => (\n                    <CartSingle\n                      key={index}\n                      data={i}\n                      quantityChangeHandler={quantityChangeHandler}\n                      removeFromCartHandler={removeFromCartHandler}\n                    />\n                  ))}\n              </div>\n            </div>\n\n            <div className=\"px-5 mb-3\">\n              {/* checkout buttons */}\n              <Link to=\"/checkout\">\n                <div\n                  className={`h-[45px] flex items-center justify-center w-[100%] bg-[#e44343] rounded-[5px]`}\n                >\n                  <h1 className=\"text-[#fff] text-[18px] font-[600]\">\n                    Оплатить (руб ₽ {totalPrice})\n                  </h1>\n                </div>\n              </Link>\n            </div>\n          </>\n        )}\n      </div>\n    </div>\n  );\n};\n\nconst CartSingle = ({ data, quantityChangeHandler, removeFromCartHandler }) => {\n  //const initialState = { value: 1 }\n  const [value, setValue] = useState(data.qty);\n  const totalPrice = data.discountPrice * value;\n\n  const increment = (data) => {\n    if (data.stock < value) {\n      toast.error(\"закончилось!\");\n    } else {\n      setValue(value + 1);\n      const updateCartData = { ...data, qty: value + 1 };\n      quantityChangeHandler(updateCartData);\n    }\n  };\n\n  const decrement = (data) => {\n    setValue(value === 1 ? 1 : value - 1);\n    const updateCartData = { ...data, qty: value === 1 ? 1 : value - 1 };\n    quantityChangeHandler(updateCartData);\n  };\n\n  return (\n    <div className=\"border-b p-4\">\n      <div className=\"w-full flex items-center\">\n        <div>\n          <div\n            className={`bg-[#e44343] border border-[#e4434373] rounded-full w-[25px] h-[25px] ${styles.noramlFlex} justify-center cursor-pointer`}\n            onClick={() => increment(data)}\n          >\n            <HiPlus size={18} color=\"#fff\" />\n          </div>\n          <span className=\"pl-[10px]\">{data.qty}</span>\n          <div\n            className=\"bg-[#a7abb14f] rounded-full w-[25px] h-[25px] flex items-center justify-center cursor-pointer\"\n            onClick={() => decrement(data)}\n          >\n            <HiOutlineMinus size={16} color=\"#7d879c\" />\n          </div>\n        </div>\n        <img\n          src={`${data?.images[0]?.url}`}\n          alt=\"\"\n          className=\"w-[130px] h-min ml-2 mr-2 rounded-[5px]\"\n        />\n        <div className=\"pl-[5px]\">\n          <h1>{data.name}</h1>\n          <h4 className=\"font-[400] text-[15px] text-[#00000082]\">\n            ₽{data.discountPrice} * {value}\n          </h4>\n          <h4 className=\"font-[600] text-[17px] pt-[3px] text-[#d02222] font-Roboto\">\n            ₽{totalPrice}\n          </h4>\n        </div>\n        <RxCross1\n          className=\"cursor-pointer\"\n          onClick={() => removeFromCartHandler(data)}\n        />\n      </div>\n    </div>\n  );\n};\n\nexport default Cart;\n","/data/data/com.termux/files/home/commerce/frontend/src/components/Wishlist/Wishlist.jsx",["907","908","909","910"],[],"import React, { useState } from \"react\";\nimport { RxCross1 } from \"react-icons/rx\";\nimport { IoBagHandleOutline } from \"react-icons/io5\";\nimport { BsCartPlus } from \"react-icons/bs\";\nimport styles from \"../../styles/styles\";\n