<span className=\"font-[500]\">\n                  {products && products.length}\n                </span>\n              </h5>\n              <h5 className=\"font-[600] pt-3\">\n                Всего отзывов:{\" \"}\n                <span className=\"font-[500]\">{totalReviewsLength}</span>\n              </h5>\n                <Link to={`/shop/preview/${data?.shop._id}`}>\n                <div\n                  className={`${styles.button} !rounded-[4px] !h-[39.5px] mt-3`}\n                >\n                  <h4 className=\"text-white\">Посетить магазин</h4>\n                </div>\n              </Link>\n            </div>\n          </div>\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default ProductDetails;\n","/data/data/com.termux/files/home/commerce/frontend/src/components/Route/FeaturedProduct/FeaturedProduct.jsx",["739"],[],"import React, { useEffect } from \"react\";\nimport { useSelector } from \"react-redux\";\nimport styles from \"../../../styles/styles\";\nimport ProductCard from \"../ProductCard/ProductCard\";\n\nconst FeaturedProduct = () => {\n  const {allProducts} = useSelector((state) => state.products);\n   \n  return (\n    <div>\n      <div className={`${styles.section}`}>\n        <div className={`${styles.heading}`}>\n          <h1>Популярные товары</h1>\n        </div>\n        <div className=\"grid grid-cols-1 gap-[20px] md:grid-cols-2 md:gap-[25px] lg:grid-cols-4 lg:gap-[25px] xl:grid-cols-5 xl:gap-[30px] mb-12 border-0\">\n        {\n            allProducts && allProducts.length !== 0 &&(\n              <>\n               {allProducts && allProducts.map((i, index) => <ProductCard data={i} key={index} />)}\n              </>\n            )\n           }\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default FeaturedProduct;\n","/data/data/com.termux/files/home/commerce/frontend/src/components/Profile/ProfileSidebar.jsx",["740"],[],"/data/data/com.termux/files/home/commerce/frontend/src/components/Profile/Track